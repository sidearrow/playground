{
  "compilerOptions": {
    "moduleResolution": "node",
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "ESNext",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "sourceMap": true,
    "strictNullChecks": true,
    "noUnusedLocals": true
  }
}
